<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="444.0" prefWidth="383.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.LoginController">
    <Label layoutX="82.0" layoutY="14.0" text="Inicio de Sesion">
        <font>
            <Font name="System Bold" size="27.0"/>
        </font>
    </Label>
    <Separator layoutX="14.0" layoutY="56.0" prefHeight="8.0" prefWidth="351.0"/>
    <Label layoutX="109.0" layoutY="81.0" text="Ingrese el Usuario :" textFill="#1f1dbc">
        <font>
            <Font name="System Bold" size="18.0"/>
        </font>
    </Label>
    <Label layoutX="95.0" layoutY="187.0" text="Ingrese la Contraseña :" textFill="#4e17c3">
        <font>
            <Font name="System Bold Italic" size="18.0"/>
        </font>
    </Label>
    <TextField fx:id="txtUsuario" layoutX="38.0" layoutY="116.0" prefHeight="25.0" prefWidth="292.0"/>
    <PasswordField fx:id="txtContraseña" layoutX="33.0" layoutY="222.0" prefHeight="25.0" prefWidth="292.0"/>
    <Button fx:id="btnIniciarSesion" layoutX="85.0" layoutY="343.0" mnemonicParsing="false"
